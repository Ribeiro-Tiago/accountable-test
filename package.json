{
	"name": "accountable-test",
	"version": "1.0.0",
	"description": "test for full stack dev position at accountable.eu",
	"main": "server.ts",
	"scripts": {
		"start": "nodemon --watch 'src/**/*' -e ts,tsx --exec ts-node ./src/server.ts",
		"start:test": "npm start && jest --watch",
		"test": "jest --watch",
		"test:once": "jest",
		"docker": "docker-compose up",
		"docker:build": "docker-compose up --build",
		"docker:no-test": "npm run docker-api-bash && npm start",
		"docker:api-bash": "docker exec -it api bash",
		"docker:api-logs": "docker logs api -t -f",
		"docker:db-bash": "docker exec -it api bash",
		"docker:db-logs": "docker logs db -t -f"
	},
	"dependencies": {
		"body-parser": "^1.19.0",
		"cors": "^2.8.5",
		"express": "^4.17.1",
		"helmet": "^3.21.2",
		"mongoose": "^5.7.9",
		"morgan": "^1.9.1",
		"node-fetch": "^2.6.0"
	},
	"devDependencies": {
		"@types/body-parser": "^1.17.1",
		"@types/cors": "^2.8.6",
		"@types/dotenv": "^6.1.1",
		"@types/express": "^4.17.2",
		"@types/helmet": "0.0.45",
		"@types/jest": "^24.0.19",
		"@types/mongoose": "^5.5.30",
		"@types/morgan": "^1.7.37",
		"@types/node-fetch": "^2.5.2",
		"@types/supertest": "^2.0.8",
		"dotenv": "^8.2.0",
		"jest": "^24.9.0",
		"nodemon": "^1.19.4",
		"supertest": "^4.0.2",
		"ts-jest": "^24.1.0",
		"ts-node": "^8.4.1",
		"typescript": "^3.6.4"
	},
